    "superagent": "^3.8.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-node": "6.0.1",
    "husky": "^1.3.1",
    "istanbul": "0.4.5",
    "mocha": "5.0.5",
    "nock": "^9.6.1"
