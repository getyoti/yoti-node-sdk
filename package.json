{
  "name": "yoti",
  "version": "4.1.0",
  "description": "Yoti NodeJS SDK for back-end integration",
  "author": "Yoti LTD <tech@yoti.com> (https://www.yoti.com/developers)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/getyoti/yoti-node-sdk.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "npx eslint *.js './src/**/*.js' './tests/**/*.spec.js' config/*.js",
    "test": "jest",
    "pre-commit": "npm run test"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./*.js",
      "./src/**/*.js",
      "./config/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "./coverage"
  },
  "dependencies": {
    "node-forge": "1.3.0",
    "protobufjs": "7.1.0",
    "superagent": "8.0.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.0.1",
    "eslint": "8.23.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.2",
    "eslint-plugin-node": "11.1.0",
    "husky": "8.0.1",
    "jest": "29.0.1",
    "nock": "13.2.9"
  }
}
